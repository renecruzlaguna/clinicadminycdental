
{% if queries   %}
    <h3>Consultas registradas para esta fecha</h3>
<table class="table table-striped table-bordered table-hover dataTables-example">
    <thead>
    <tr>
        <th>Nombre del cliente</th>

        <th>Fecha de la consulta</th>
        <th>Hora inicial</th>
        <th>Hora final</th>

    </tr>
    </thead>
    <tbody>
    {% for esp in queries %}
        <tr>


            <td>{{ esp.usuarioRegistro.getCompleteName() }}</td>


            <td>{{ esp.dia }}/{{ esp.mes }}/{{ esp.anno }}</td>
            <td>{{ esp.horaInicialC }}:{{ esp.minutoInicialC }}</td>
            <td>{{ esp.horaFinalC }}:{{ esp.minutoFinalC }}</td>

        </tr>
    {% endfor %}
    </tbody>
</table>
    {% else %}
        <h3>No existen consultas registradas para esta fecha</h3>
{% endif %}